{
  "manifest_version": 2,
  "name": "Linked in Feed Remover",
  "version": "1.0",
  "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/feed/*"],
        "js": ["popup.js"]
      }
    ],
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab", "tabs"
   ]
}
